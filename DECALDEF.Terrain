fader LiquidFader
{
   DecayStart 3
   DecayTime 6
}

fader LiquidFaderFast
{
   DecayStart 3
   DecayTime 4
}

slider Drops
{
   DistX 0
   DistY -320
   SlideStart 0.01
   SlideTime 5
}

stretcher waterstretcher
{
   GoalY 2.8
   StretchStart 0
   StretchTime 3
}

Combiner WaterDropEffect
{
   LiquidFaderFast
   Drops
   waterstretcher
}

// Water

decal WaterDrops1
{
	pic WSPL1A
	add 0.3
	x-scale 0.4
	y-scale 0.4
	randomflipx
	animator WaterDropEffect
}

